{
  "name": "@e1011/i18n-kit",
  "version": "1.0.18",
  "main": "./dist/lib/src/index.js",
  "module": "./dist/lib/esm/index.js",
  "sideEffects": false,
  "types": "dist/types/src/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/edgar0011/e1011-i18n-kit"
  },
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "import": "./dist/lib/esm/index.js",
      "require": "./dist/lib/src/index.js",
      "types": "./dist/types/src/index.d.ts"
    },
    "./react": {
      "import": "./dist/react/esm/src/react/index.js",
      "require": "./dist/react/index.js",
      "types": "./dist/types/src/react/index.d.ts"
    },
    "./locales": {
      "import": "./dist/locales/esm/index.js",
      "require": "./dist/locales/index.js",
      "types": "./dist/types/src/locales/index.d.ts"
    },
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "scripts": {
    "prepare": "husky install",
    "prepublish": "yarn clean && yarn build:lib",
    "clean": "rimraf ./dist ./es ./coverage",
    "build:lib": "yarn tsc && BABEL_ENV=production NODE_ENV=production rollup --config --environment INCLUDE_DEPS,BUILD:production",
    "build": "yarn prepush && yarn build:lib",
    "watch": "rollup --config --watch",
    "lint": "eslint src",
    "lint:ts": "eslint src --ext js,jsx,ts,tsx",
    "lint:quiet": "eslint src --quiet",
    "lint:ts:quiet": "eslint src --ext js,jsx,ts,tsx --quiet",
    "lint:fix": "yarn lint --fix",
    "test": "jest ./src --verbose",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "tsc": "tsc",
    "prepush": "yarn lint:ts && yarn test && yarn tsc"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/eslint-plugin": "^7.14.5",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-decorators": "^7.16.4",
    "@babel/plugin-proposal-optional-chaining": "^7.16.0",
    "@babel/plugin-proposal-private-methods": "^7.16.0",
    "@babel/plugin-proposal-private-property-in-object": "^7.16.0",
    "@babel/plugin-syntax-optional-chaining": "^7.8.3",
    "@babel/plugin-transform-async-to-generator": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.0",
    "@earlorg/eslint-config": "^1.1.2",
    "@types/jest": "^27.0.3",
    "@types/lodash-es": "^4.17.5",
    "@types/ramda": "^0.27.62",
    "@types/react": "^17.0.38",
    "@types/sanitize-html": "^2.6.1",
    "@types/styled-components": "^5.1.19",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "babel-jest": "^27.3.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-react-html-attrs": "^3.0.5",
    "babel-plugin-styled-components": "^2.0.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-import-resolver-typescript": "^3.5.3",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "prettier-eslint": "^15.0.1",
    "prettier-eslint-cli": "^7.1.0",
    "reflect-metadata": "^0.1.13",
    "rollup": "^2.60.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "core-js": "^3.20.2",
    "i18next": "^21.6.14",
    "react-i18next": "^11.16.2"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!lodash-es)",
      "<rootDir>/dist"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|css|pdf)$": "<rootDir>/test/file-mock.js",
      "^lodash-es$": "lodash",
      "^lodash-es/(.*)": "lodash/$1"
    },
    "verbose": true,
    "transform": {
      "^.+\\.jsx$": "babel-jest",
      "^.+\\.js$": "babel-jest",
      "^.+\\.ts$": "babel-jest",
      "^.+\\.tsx$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "coverageDirectory": "report",
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "setupFiles": [
      "<rootDir>/test/testSetup.js"
    ]
  }
}
